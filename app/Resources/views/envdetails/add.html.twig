{% extends "layout.html.twig" %}

{% block title %}Add a new details of an environment{% endblock %}
{% block body %}
 <div class="container">
{{ form_start(form) }}
{{ form_errors(form) }}
    {{ form_row(form.servercategory) }}
    {{ form_row(form.description, {'required': false}) }}
    {{ form_row(form.environment) }}
    {{ form_row(form.server) }}
    {{ form_row(form.user) }}
    {{ form_row(form.parameter) }}
    {{ form_row(form.save) }}

{#    <button type="submit">create !</button> #}
{{ form_end(form) }}
    
</div>
{%  endblock body %}